{
  "name": "@ubersetzer/cli",
  "version": "1.0.0",
  "author": "Manuel Heidrich @mahnuh",
  "bin": {
    "ubersetzer": "./bin/run.js"
  },
  "dependencies": {
    "@inquirer/prompts": "7.3.3",
    "@oclif/core": "4.2.10",
    "@oclif/plugin-help": "6.2.26",
    "@oclif/plugin-plugins": "5.4.34",
    "axios": "1.8.3",
    "conf": "13.1.0",
    "yauzl": "3.2.0"
  },
  "description": "Ubersetzer CLI",
  "devDependencies": {
    "@oclif/test": "4.1.12",
    "@types/chai": "5.2.0",
    "@types/mocha": "10.0.10",
    "@types/node": "22.13.10",
    "@types/yauzl": "2.10.3",
    "@typescript-eslint/eslint-plugin": "8.26.1",
    "@typescript-eslint/parser": "8.26.1",
    "@typescript-eslint/utils": "8.26.1",
    "chai": "5.2.0",
    "eslint": "9.22.0",
    "eslint-config-oclif": "6.0.28",
    "eslint-config-oclif-typescript": "3.1.14",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-class-members": "1.21.0",
    "mocha": "11.1.0",
    "oclif": "4.17.34",
    "shx": "0.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ubersetzerio/cli",
  "license": "MIT",
  "main": "",
  "oclif": {
    "bin": "ubersetzer",
    "dirname": "ubersetzer",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "repository": "ubersetzerio/cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "bugs": "https://github.com/ubersetzerio/cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
