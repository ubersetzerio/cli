{
  "name": "@ubersetzer/cli",
  "description": "Ubersetzer CLI",
  "version": "0.1.1",
  "author": "Manuel Heidrich",
  "bin": {
    "ubersetzer": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "bugs": "https://github.com/ubersetzerio/cli/issues",
  "dependencies": {
    "@oclif/command": "1.8.16",
    "@oclif/config": "1.18.3",
    "@oclif/plugin-help": "5.1.11",
    "axios": "0.25.0",
    "cli-ux": "5.6.6",
    "inquirer": "6.5.2",
    "tslib": "1.14.1",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.10",
    "@oclif/test": "2.0.3",
    "@types/chai": "4.3.0",
    "@types/inquirer": "8.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.18.0",
    "@types/yauzl": "2.9.2",
    "chai": "4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^3.1.2",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ubersetzerio/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ubersetzer",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ubersetzerio/cli",
  "types": "lib/index.d.ts"
}
